{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "apps/web/src/app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "CI": "true",
    "VERCEL": "true",
    "SKIP_HUSKY": "true"
  },
  "framework": "nextjs",
  "installCommand": "npm install",
  "buildCommand": "cd apps/web && npm run build",
  "outputDirectory": "apps/web/.next"
}
